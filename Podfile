# platform :ios, '9.0'

target 'PPC' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for PPC
  pod 'Firebase/Core'
  pod 'Firebase/Storage'
  pod 'Firebase/Auth'
  pod 'Firebase/Messaging'
  pod 'Firebase/Firestore'

  target 'PPCTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'PPCUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
